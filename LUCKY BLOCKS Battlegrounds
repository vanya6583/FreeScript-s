local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("üî• LAVA ‚ùì LUCKY BLOCKS Battlegrounds", "Midnight")

local Tab = Window:NewTab("Charecter") -- Tab Charecter

local Section = Tab:NewSection("Walk Speed") -- Section Speed

Section:NewSlider("Speed Power", "Min -16 Max -220", 220, 16, function(NNS) -- Speed Power
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = NNS
end)
local Section = Tab:NewSection("Jump Power") -- Section Jump

Section:NewSlider("Jump Power", "Min -50 Max -220", 220, 50, function(NNJ) -- Jump Power
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = NNJ
end)

local Section = Tab:NewSection("Infinity Jump") -- Section InfJump

Section:NewToggle("Infinity Jump", "Jum Jum Yummy", function(state) -- Infinity Jump
    if state then
                _G.infinjump = true
 
    local Player = game:GetService("Players").LocalPlayer
    local Mouse = Player:GetMouse()
    Mouse.KeyDown:connect(function(k)
    if _G.infinjump then
    if k:byte() == 32 then
    Humanoid = game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
    Humanoid:ChangeState("Jumping")
 wait(0.1)
    Humanoid:ChangeState("Seated")
end
end
end)
    else
                _G.infinjump = false
 
    local Player = game:GetService("Players").LocalPlayer
    local Mouse = Player:GetMouse()
    Mouse.KeyDown:connect(function(k)
    if _G.infinjump then
    if k:byte() == 32 then
    Humanoid = game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
    Humanoid:ChangeState("Jumping")
    wait(0.1)
    Humanoid:ChangeState("Seated")
end
end
end)
    end
end)
local Tab = Window:NewTab("Give Block") -- Tab Give Block

local Section = Tab:NewSection("Infinity Jump") -- Section InfJump

Section:NewButton("Lucky Block", "ReplicatedStorage", function()
game:GetService("ReplicatedStorage").SpawnLuckyBlock:FireServer()
end)

Section:NewButton("Super Block", "ReplicatedStorage", function()
game:GetService("ReplicatedStorage").SpawnSuperBlock:FireServer()
end)

Section:NewButton("Diamond Block", "ReplicatedStorage", function()
game:GetService("ReplicatedStorage").SpawnDiamondBlock:FireServer()
end)

Section:NewButton("Raindbow Block", "ReplicatedStorage", function()
game:GetService("ReplicatedStorage").SpawnRainbowBlock:FireServer()
end)

Section:NewButton("Galaxy Block", "ReplicatedStorage", function()
game:GetService("ReplicatedStorage").SpawnGalaxyBlock:FireServer()
end)
local Tab = Window:NewTab("Give Time Block") -- Tab Give Block

local Section = Tab:NewSection("Gives [time] blocks") -- Section InfJump

Section:NewTextBox("Lucky Block", "1..2..3", function(txt1)
for i = 1, txt1 do
game:GetService("ReplicatedStorage").SpawnLuckyBlock:FireServer()
end
end)

Section:NewTextBox("Super Block", "1..2..3", function(txt2)
