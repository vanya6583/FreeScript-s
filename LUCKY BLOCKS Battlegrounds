local Tab = Window:NewTab("Loop Give Block") -- Tab Give Time Block

local Section = Tab:NewSection("loop give blocks") -- Section InfJump

Section:NewToggle("Lucky Block", "4...5...6", function(state)
    if state then
        _G.Enabled = true 
        while _G.Enabled == true do
            wait(0.1)
            game:GetService("ReplicatedStorage").SpawnLuckyBlock:FireServer()
        end
    else
        _G.Enabled = false 
        while _G.Enabled == true do
            wait(0.1)
            game:GetService("ReplicatedStorage").SpawnLuckyBlock:FireServer()
        end
    end
end)
Section:NewToggle("Super Block", "4...5...6", function(state)
    if state then
        _G.Enabled = true 
